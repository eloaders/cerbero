<div class="account-container">
  <div class="content clearfix">
    <form method="post" action="inex.r" enctype="multipart/form-data" id="validform"> 
       <h1><%Print ("Validate")%></h1>
          <div class="login-fields">
             <p><% Print ("Please browse file to publish data online") %></p>
               <div class="field">
                  <input type="file" name="uploadfile" style="font-size: 15px; height: 25px;" accept="file" size="47" maxlength="100"><br>
               </div> <!-- /field -->
          </div> <!-- /login-fields -->
          <div class="login-actions">
            <span class="login-checkbox">
              <input type="checkbox"  id="redirect" name="redirect" checked="checked" />
              <label for="redirect"><% Print ("Redirect to") %></label>
            </span>          
          </div> <!-- .actions -->
          <input type="submit" id="submit" value="Validate" name="submit" />
    </form>
  </div> <!-- /content -->  
</div> <!-- /account-container -->
<br/>
<br/>
<br/>
